{
  "extends": "eslint:recommended",
  "rules": {
    "arrow-spacing": "error",
    "arrow-parens": [
      "error",
      "always"
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "curly": [
      2,
      "multi-line"
    ],
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "keyword-spacing": [
      "error",
      {
        "after": true
      }
    ],
    "key-spacing": [
      "error",
      {
        "afterColon": true
      }
    ],
    "no-case-declarations": "off",
    "no-console": "off",
    "no-extra-boolean-cast": "off",
    "no-lonely-if": [
      "error"
    ],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 2,
        "maxEOF": 0,
        "maxBOF": 0
      }
    ],
    "no-multi-spaces": "error",
    "no-var": "error",
    "object-shorthand": [
      "error",
      "always"
    ],
    "operator-linebreak": [
      "error",
      "before"
    ],
    "one-var": [
      "error",
      "never"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "quote-props": [
      "error",
      "as-needed"
    ],
    "semi": [
      "error",
      "always"
    ],
    "space-before-blocks": "error",
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "space-infix-ops": [
      "error"
    ],
    "no-trailing-spaces": "error",
    "prefer-const": "error",
    "prefer-arrow-callback": "error"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "globals": {
    "window": true,
    "document": true,
    "console": true
  }
}
